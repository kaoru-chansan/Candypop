<h1>Admin::Customers#index</h1>
<p>Find me in app/views/admin/customers/index.html.erb</p>

<h1>全ユーザー一覧</h1>
<ul>
  <% @customers.each do |customer| %>
    <li>
      <%= customer.name %> - <%= user.email %>

      <%= if customer.is_active == true %>
        有効
      <% else %>
        退会
      <% end %>
    </li>
  <% end %>
</ul>